'''python
def LCS(x: str, y: str) -> int:
    m = len(x) + 1; n = len(y) + 1;
    C = [[0 for _ in range(n)]for _ in range(m)]
    for i in range(1, m):
        for j in range(1, n):
            if x[i-1] == y[j-1]:
                C[i][j] = C[i-1][j-1] + 1
            else:
                C[i][j] = max(C[i-1][j], C[i][j-1])
    return C[m-1][n-1]

str_x = input(); str_y = input()
print(LCS(str_x, str_y))
'''
